                    <MenuItem fx:id="newGame" mnemonicParsing="false" onAction="#chinczyk" text="Chińczyk" />
                      <MenuItem mnemonicParsing="false" text="Kółko i krzyżyk" />
                    </items>
                  </Menu>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
